{
    "name": "virhi/ui-rest-api_doctrine-bundle",
    "description": "generate a UI on top of Rest Hal APi",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Rhino",
            "email": "vincent.rhino@gmail.com",
            "role": "Developer"
        }
    ],

    "autoload": {
        "psr-4": {
            "Virhi\\UiRestApiDoctrineBundle\\": "src/Virhi/UiRestApiDoctrineBundle/"
        }
    },

    "autoload-dev": {
        "psr-4": { "Virhi\\UiRestApiDoctrineBundle\\Tests\\": "Tests/" }
    },

    "require": {
        "virhi/RestHalBundle": "@dev",
        "virhi/component": "@dev",
        "avanzu/admin-theme-bundle": "~1.1"
    },

    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },

    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/virhi/RestHalBundle.git"
        },
        {
            "type": "git",
            "url": "https://github.com/virhi/Component.git"
        }
    ]
}
