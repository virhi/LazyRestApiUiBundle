<div class="form-group">
    <label class="col-sm-2 control-label" for="{{ field.name }}">{{ field.name }} :</label>
    <div class="col-sm-10">
        <select multiple name="{{ field.name }}">
            {% for ef in embedField %}
                {% set item = attribute(ef, field.entity) %}
                {% for i in item %}
                    {% set value = attribute(i, field.field) %}
                    <option value="{{ value }}" {% if value in field.objectSelected %} selected {% endif %}>{{ value }}</option>
                {% endfor %}
            {% endfor %}
        </select>
    </div>
</div>

